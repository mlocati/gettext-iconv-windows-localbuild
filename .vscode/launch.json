{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "iconv 1.18",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceRoot}\\src\\libiconv-1.18\\build\\src\\iconv.exe",
      "args": ["--help"],
      "stopAtEntry": true,
      "cwd": "${workspaceFolder}",
      "environment": [
        {
          "name": "LANG",
          "value": "it"
        }
      ],
      "MIMode": "gdb",
      "miDebuggerPath": "${workspaceRoot}\\cygwin\\bin\\gdb.exe",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "xgettext 0.25.1",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceRoot}\\src\\gettext-0.25.1\\build\\gettext-tools\\src\\xgettext.exe",
      "args": ["--help"],
      "stopAtEntry": true,
      "cwd": "${workspaceFolder}",
      "environment": [
        {
          "name": "LANG",
          "value": "it"
        }
      ],
      "MIMode": "gdb",
      "miDebuggerPath": "${workspaceRoot}\\cygwin\\bin\\gdb.exe",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "spit 1.0-pre2 (64-static)",
      "type": "cppdbg",
      "request": "launch",
      "externalConsole": true,
      "program": "${workspaceRoot}\\64-static\\src\\gettext-1.0-pre2\\build\\gettext-tools\\src\\spit.exe",
      "additionalSOLibSearchPath": "${workspaceRoot}\\64-static\\installed\\bin",
      "args": [
        "--species=ollama",
        "--url=http://localhost:11434",
        "--model=llama3.2:3b",
        "--to=it",
      ],
      "cwd": "${workspaceRoot}\\64-static\\installed\\bin",
      "stopAtEntry": true,
      "targetArchitecture": "x64",
      "environment": [
        {
          "name": "LANG",
          "value": "it"
        }
      ],
      "MIMode": "gdb",
      "miDebuggerPath": "${workspaceRoot}\\cygwin\\bin\\gdb.exe",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}
