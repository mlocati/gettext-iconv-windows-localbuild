{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "iconv 1.18",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceRoot}\\src\\libiconv-1.18\\build\\src\\iconv.exe",
      "args": ["--help"],
      "stopAtEntry": true,
      "cwd": "${workspaceFolder}",
      "environment": [
        {
          "name": "LANG",
          "value": "it"
        }
      ],
      "MIMode": "gdb",
      "miDebuggerPath": "C:\\cygwin\\bin\\gdb.exe",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}
